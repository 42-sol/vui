{
  "name": "@42sol/vui",
  "private": false,
  "version": "0.1.2",
  "homepage": "http://42-sol.github.io/vui",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/vui.umd.js",
  "module": "./dist/vui.es.js",
  "exports": {
    ".": {
      "import": "./dist/vui.es.js",
